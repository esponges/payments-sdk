{
  "name": "payments-sdk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"npm run serve:dev\"",
    "build": "tsc -b && vite build",
    "serve": "npm run build && npx tsx server/app.ts",
    "serve:dev": "npx tsx --watch server/app.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "vite": "vite"
  },
  "dependencies": {
    "express": "^4.21.0",
    "post-robot": "^8.0.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/post-robot": "^10.0.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "install": "^0.13.0",
    "npm": "^10.9.0",
    "sass": "^1.79.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
